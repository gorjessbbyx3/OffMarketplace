entrypoint = "main.py"
modules = ["python-3.10:v18-20230807-322e88b"]
run = "streamlit run main.py"

hidden = [".pythonlibs"]

[nix]
channel = "stable-23_05"

[deployment]
deploymentTarget = "gce"
run = "streamlit run main.py"

[workflows]
runButton = "Start Server"

[[workflows.workflow]]
name = "Start Server"
author = 43369170
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node server.js"

